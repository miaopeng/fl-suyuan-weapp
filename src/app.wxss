.loading {
  margin: 0 auto;
  text-align: center;
  font-size: 32rpx;
  vertical-align: middle;
}

.loading .icon-loading {
  margin-right: 20rpx;
}

.icon-loading {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  vertical-align: middle;
  animation: loading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

