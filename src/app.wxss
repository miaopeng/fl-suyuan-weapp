@-webkit-keyframes loading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

@keyframes loading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

.cell-row {
  padding: 10rpx 40rpx;
  background-color: #fff;
  color: #666;
  font-size: 28rpx;
  border-bottom: 2rpx solid #e5e5e5;
  border-radius: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.cell-row:first-child {
  border-radius: 6rpx 6rpx 0 0;
}

.cell-row:last-child {
  border-radius: 0 0 6rpx 6rpx;
  border-bottom: 0;
}

.cell-row::after {
  border: 0;
}

.cell-row .ft {
  position: relative;
  padding-right: 20rpx;
}

.cell-row .ft::after {
  content: ' ';
  display: inline-block;
  height: 12rpx;
  width: 12rpx;
  border-width: 4rpx 4rpx 0 0;
  border-color: #c8c8cd;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: relative;
  top: -4rpx;
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 4rpx;
}

.btn {
  width: 100%;
  color: #333;
  border-radius: 0;
  font-size: 32rpx;
}

.btn.white {
  color: white;
}

.btn.noborder {
  line-height: 32rpx;
  width: auto;
  border: 0;
}

.btn.moon {
  border-radius: 50%;
  width: 30vw;
  height: 30vw;
  text-align: center;
  line-height: 30vw;
  background-color: white;
  border: 0;
  color: #669;
  font-weight: bold;
}
